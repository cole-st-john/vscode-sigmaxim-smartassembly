{
    "comments": {
        "lineComment": "!",
        "blockComment": [
            "/*",
            "*/"
        ]
    },
    "brackets": [
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "autoClosingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "\"",
            "\""
        ],
        [
            "'",
            "'"
        ]
    ],
    "folding": {
        "markers1": {
            "start": "\\/\\*",
            "end": "\\*\\/"
        }
    },
    "explicitFolding.rules": [
        {
            "beginRegex": "^\\s*(ASSEMBLE)\\s*.*$",
            "endRegex": "^\\s*(END_ASSEMBLE)\\s*.*$"
        }
    ],
    "indentationRules": {
        "increaseIndentPattern": "^\\s*(ADD_CONSTRAINTS|ASSEMBLE|BEGIN_ASM_DESCR|BEGIN_CATCH_ERROR|BEGIN_COMP_DESCR|BEGIN_CONSTRAINT_SET|BEGIN_DEBUG|BEGIN_DEP_DESCR|BEGIN_GUI_DESCR|BEGIN_SIGNATURE|BEGIN_SUBCOMP|BEGIN_SUBCOMP_INFO|BEGIN_TOPELEM_INFO|CALL_EX|CREATE_DRW_SYMBOL|CREATE_DRW_VIEW_SYMBOL|CREATE_FEATURE_RELATION|CREATE_GENERAL_MERGE|CREATE_RELATION|CREATE_SYMBOL|CREATE_UDF|ELSE|FOR|IF|REMOVE_FEATURE_RELATION|REMOVE_RELATION|WHILE).*$",
        "decreaseIndentPattern": "^\\s*(END|ELSE).*$"
    }
}